{
  "presets": [
    "@babel/preset-typescript",
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "modules": "commonjs",
        "targets": {
          "browsers": "last 2 versions"
        }
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "pure": true
      }
    ]
  ],
  "env": {
    "es": {
      "presets": [
        "@babel/preset-typescript",
        "@babel/preset-react",
        [
          "@babel/preset-env",
          {
            "modules": "false",
            "targets": {
              "browsers": "last 2 versions"
            }
          }
        ]
      ],
      "plugins": [
        [
          "babel-plugin-styled-components",
          {
            "pure": true
          }
        ]
      ]
    }
  }
}
